<%- include('common/head') -%>
    <!-- App Header -->
    <div class="appHeader bg-primary text-light">
        <div class="left">
            <a href="javascript:;" class="headerButton goBack">
                <ion-icon name="chevron-back-outline"></ion-icon>
            </a>
        </div>
        <div class="pageTitle">Books Details</div>
        <div class="right">
        </div>
    </div>
    <!-- * App Header -->


    <!-- App Capsule -->
    <div id="appCapsule">
        <ul class="listview image-listview media mb-2">
            <% Object.keys(book_details).forEach(function(prop) { %>
                <li>
                    <a href="/book-open/<%- book_details[prop]['book_id'] %>/<%- book_details[prop]['id'] %>" class="item">

                        <div class="in">
                            <div>
                                <%- book_details[prop]['lession_title'] %>

                            </div>

                        </div>
                        <div class="imageWrapper">
                            <img src="/views/assets/img/app-assets/bookmark.png" alt="image" class="imaged w36">
                        </div>
                    </a>
                </li>
                <% }); %>

        </ul>


    </div>
    <!-- * App Capsule -->

    <%- include('common/script') -%>