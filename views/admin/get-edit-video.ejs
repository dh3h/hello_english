<%- include('common/head') -%>
  <%- include('common/header'); %>
    <%- include('common/sidebar'); %>

      <div class="content-body">
        <!-- row -->
        <div class="container-fluid">
          <!-- row -->
          <div class="row">

            <div class="col-xl-12 col-lg-8">
              <div class="card profile-card card-bx m-b30">
                <div class="card-header">
                  <h6 class="title">Edit Videos</h6>
                </div>
                <form class="profile-form">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Video title</label>
                        <input type="text" name="vid_title" class="form-control" placeholder="Enter Video title">
                      </div>

                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Youtube Link</label>
                        <input type="text" name="youtube_link" class="form-control" placeholder="Enter Youtube Link">
                      </div>
                      
                      <div class="col-sm-12 m-b30">
                        <label class="form-label">Questions</label>
                        <textarea name="questions" id="about_discriptions" class="form-control" rows="10" cols="10"></textarea>
                      </div>

                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Options 1 :</label>
                        <input type="text" name="Options1" class="form-control" placeholder="Enter Options 1">
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Options 2 :</label>
                        <input type="text" name="Options2" class="form-control" placeholder="Enter Options 2">
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Options 3 :</label>
                        <input type="text" name="Options3" class="form-control" placeholder="Enter Options 3">
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Options 4 :</label>
                        <input type="text" name="Options4" class="form-control" placeholder="Enter Options 4">
                      </div>
                      <div class="col-sm-6 m-b30">
                        <label class="form-label">Correct Answer :</label>
                        <input type="text" name="Options4" class="form-control" placeholder="Enter Options 4">
                      </div>
                      
                    </div>
                  </div>
                  <div class="card-footer">
                    <button class="btn btn-primary">Submit</button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>

      <%- include('common/footer') -%>
        <%- include('common/script'); %>

          <script>
            $(document).ready(function () {
              var table = $('#userListTable').DataTable({
                responsive: true,
                language: {
                  paginate: {
                    next: '<i class="fa fa-angle-double-right" aria-hidden="true"></i>',
                    previous: '<i class="fa fa-angle-double-left" aria-hidden="true"></i>'
                  }
                }
              });
            })
          </script>