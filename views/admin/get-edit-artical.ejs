<%- include('common/head') -%>
  <%- include('common/header'); %>
    <%- include('common/sidebar'); %>

      <div class="content-body">
        <!-- row -->
        <div class="container-fluid">
          <!-- row -->
          <div class="row">

            <div class="col-xl-12 col-lg-8">
              <div class="card profile-card card-bx m-b30">
                <div class="card-header">
                  <h6 class="title">Edit Tips List</h6>
                </div>
                <form class="profile-form">
                  <div class="card-body">
                    <div class="row">
                      <div class="col-sm-12 m-b30">
                        <label class="form-label">Tips title</label>
                        <input type="text" class="form-control" placeholder="Enter Your Tips title">
                      </div>

                      <div class="col-sm-12 m-b30">
                        <label class="form-label">Youtube Link </label>
                        <input type="text" class="form-control" placeholder="Enter Youtube Link">
                      </div>
                      
                    </div>
                  </div>
                  <div class="card-footer">
                    <button class="btn btn-primary">Submit</button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>

      <%- include('common/footer') -%>
        <%- include('common/script'); %>

          <script>
            $(document).ready(function () {
              var table = $('#userListTable').DataTable({
                responsive: true,
                language: {
                  paginate: {
                    next: '<i class="fa fa-angle-double-right" aria-hidden="true"></i>',
                    previous: '<i class="fa fa-angle-double-left" aria-hidden="true"></i>'
                  }
                }
              });
            })
          </script>