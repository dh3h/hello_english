<%- include('common/head') -%>
    <%- include('common/header'); %>
        <%- include('common/sidebar'); %>

            <div class="content-body">
                <!-- row -->
                <div class="container-fluid">
                    <!-- row -->
                    <div class="row">

                        <div class="col-xl-12 col-lg-8">
                            <div class="card profile-card card-bx m-b30">
                                <div class="card-header">
                                    <h6 class="title">Add Fill the code from video tips</h6>
                                </div>
                                <form class="profile-form">
                                    <div class="card-body">
                                        <div class="row">
                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Select Phace</label>
                                                
                                                <select name="select_Phace" class="form-control" id="">
                                                    <option value="" selected disabled> Select Phases </option>
                                                    <option value="">Phace 1</option>
                                                    <option value="">Phace 2</option>
                                                    <option value="">Phace 3</option>
                                                    <option value="">Phace 4</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Select Lesson</label>
                                                <select name="select_lessions" class="form-control" id="">
                                                    <option value="" selected disabled> Select Lessons </option>
                                                    <option value="">Lessons 1</option>
                                                    <option value="">Lessons 2</option>
                                                    <option value="">Lessons 3</option>
                                                    <option value="">Lessons 4</option>
                                                </select>
                                            </div>
                                            <div class="col-sm-12 m-b30">
                                                <label class="form-label">Questions</label>
                                                <textarea name="Questions" id="about_discriptions" class="form-control"
                                                    rows="10" cols="10"></textarea>
                                            </div>

                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Options 1</label>
                                                <input type="text" name="options1" class="form-control"
                                                    placeholder="Enter Options 1">
                                            </div>

                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Options 2</label>
                                                <input type="text" name="options2" class="form-control"
                                                    placeholder="Enter Options 2">
                                            </div>

                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Options 3</label>
                                                <input type="text" name="options3" class="form-control"
                                                    placeholder="Enter Options 3">
                                            </div>

                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Options 4</label>
                                                <input type="text" name="options4" class="form-control"
                                                    placeholder="Enter Options 4">
                                            </div>

                                            <div class="col-sm-6 m-b30">
                                                <label class="form-label">Write Correct Option</label>
                                                <input type="text" name="options4" class="form-control"
                                                    placeholder="Enter Correct Option">
                                            </div>

                                        </div>
                                    </div>
                                    <div class="card-footer">
                                        <button class="btn btn-primary">Submit</button>
                                    </div>
                                </form>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

            <%- include('common/footer') -%>
                <%- include('common/script'); %>

                    <script>
                        $(document).ready(function () {
                            var table = $('#userListTable').DataTable({
                                responsive: true,
                                language: {
                                    paginate: {
                                        next: '<i class="fa fa-angle-double-right" aria-hidden="true"></i>',
                                        previous: '<i class="fa fa-angle-double-left" aria-hidden="true"></i>'
                                    }
                                }
                            });
                        })
                    </script>