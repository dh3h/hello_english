<%- include('common/head') -%>
    <!-- App Header -->
    <div class="appHeader bg-primary text-light">
        <div class="left">
            <a href="javascript:;" class="headerButton goBack">
                <ion-icon name="chevron-back-outline"></ion-icon>
            </a>
        </div>
        <div class="pageTitle">Videos</div>
        <div class="right">
        </div>
    </div>
    <!-- * App Header -->

    <!-- App Capsule -->
    <div id="appCapsule">
        <ul class="listview image-listview media mb-2">
            <% Object.keys(video_lists).forEach(function(prop) { %>
                <li>
                    <a href="/videos-details/<%- video_lists[prop]['id'] %>" class="item">
                        <div class="imageWrapper">
                            <img src="/views/assets/img/app-assets/videos.png" alt="image" class="imaged w64">
                        </div>
                        <div class="in">
                            <div class="mb-3">
                                <%- video_lists[prop]['video_title'] %>
                                <!-- <div class="text-muted">Top. easy </div> -->
                                <span class="badge badge-primary"><span class="convert_date"><%- video_lists[prop]['date'] %></span>. 30coins</span>
                            </div>

                        </div>
                    </a>
                </li>
                <% }); %>

        </ul>


    </div>
    <!-- * App Capsule -->


    <%- include('common/script') -%>