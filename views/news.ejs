<%- include('common/head') -%>
    <!-- App Header -->
    <div class="appHeader bg-primary text-light">
        <div class="left">
            <a href="javascript:;" class="headerButton goBack">
                <ion-icon name="chevron-back-outline"></ion-icon>
            </a>
        </div>
        <div class="pageTitle">News</div>
        <div class="right">
        </div>
    </div>
    <!-- * App Header -->

    <!-- App Capsule -->
    <div id="appCapsule">
        <ul class="listview image-listview media mb-2">
            <% Object.keys(News_list_app).forEach(function(prop) { %>
                <li>
                    <a href="/new-details/<%- News_list_app[prop]['id'] %>" class="item">
                        <div class="imageWrapper">
                            <img src="/views/assets/img/app-assets/new-1.png" alt="image" class="imaged w64">
                        </div>
                        <div class="in">
                            <div>
                                <%- News_list_app[prop]['news_title'] %>
                                <div class="text-muted">Top. Moderate </div>
                                <span class="badge badge-primary"><span class="convert_date"><%- News_list_app[prop]['date'] %></span> 30coins</span>
                            </div>

                        </div>
                    </a>
                </li>
                <% }); %>

        </ul>


    </div>
    <!-- * App Capsule -->
    <%- include('common/script') -%>